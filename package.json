{
  "name": "net-monitoring",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "server": "nodemon ./bin/www",
    "build": "npx pkg . --debug"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "ping": "^0.4.4"
  },
  "devDependencies": {
    "dotenv": "^16.4.5",
    "concurrently": "^9.0.1",
    "nodemon": "^3.1.7"
  },
  "bin": "./bin/www",
  "pkg": {
    "scripts": ["bin"],
    "targets": ["win"],
    "assets": ["public/*.html", "public/stylesheets/*"],
    "outputPath": "dist"
  }
}
